<html lang="en-gb">
<%- include('_head') -%>
<%- include('_header') -%>
<body>
<h1>Users</h1>
<ol>
    <% results.forEach(function(user) {%>
        <li>
            <img 
            src ="<%= user.profileImageUrl %>"
            width ="100"
            >
            <br>
            <a href = http://localhost:3001/users/<%= user.id %>/ ><%= user.name %></a> 
            userId : <%= user.id %>
        </li>
    <% }); %>
</ol>
<%if (previous) { %>
    <a href="<%= previous %>">Previous</a>
<% } %>
<%if (next) { %>
    <a href="<%= next %>">Next</a>
<% } %>

</body>
<%- include('_footer') -%>
</html>