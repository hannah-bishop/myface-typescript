<html lang="en-gb">
<%- include('_head') -%>
<%- include('_header') -%>
<body>
<h1 class="center-text">Users</h1>
<ul>
    <% results.forEach(function(user) {%>
        <li>
            
            <p><a href = http://localhost:3001/users/<%= user.id %>/ ><%= user.name %></a> </p>
            <br>
            <img 
            src ="<%= user.profileImageUrl %>"
            width ="100"
            >
            <br>
            
        </li>
    <% }); %>
</ul>
<%if (previous) { %>
    <a href="<%= previous %>">Previous</a>
<% } %>
<%if (next) { %>
    <a href="<%= next %>">Next</a>
<% } %>

</body>
<%- include('_footer') -%>
</html>